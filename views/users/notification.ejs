<%- include('../partials/header') %>

<div class = 'container'>
  <div class = 'row'>
    <h1 style="text-align: center;">Notifications</h1>
    <div style="width: 50%; margin: 0 auto;">
      <ul>
        <% all_notifications.forEach(function(notification) { %>
          <li>
            <a href = '/campgrounds/<%= notification.campgroundSlug %>'><%= notification.username %> created a campground</a>
          </li>
        <% }) %>
      </ul>
    </div>
  </div>
  
</div>

<%- include('../partials/footer') %>